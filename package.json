{
    "name": "sqf-wiki",
    "displayName": "SQF Wiki",
    "description": "Want to look up some SQF functions? Now you can, with just a single click!",
    "version": "1.0.0",
    "publisher": "EelisLynne",
    "repository": {
        "type": "git",
        "url": "https://github.com/eelislynne/SQF-Wiki-VSC.git"
    },
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.showWiki"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.showWiki",
                "title": "Show on SQF Wiki"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "extension.showWiki",
                    "group": "1_modification",
                    "when": "editorLangId == sqf"
                }
            ]
        },
        "keybindings": [
            {
                "command": "extension.showWiki",
                "key": "ctrl+shift+q",
                "mac": "cmd+shift+q",
                "when": "editorLangId == sqf"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.10.11",
        "eslint": "^8.39.0",
        "typescript": "^2.9.2",
        "vscode": "^1.1.37"
    }
}